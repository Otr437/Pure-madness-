echo         function showTab(tabId^) {
echo             document.querySelectorAll('.tab-content'^).forEach(tab =^> tab.classList.remove('active'^)^);
echo             document.querySelectorAll('.tab-button'^).forEach(btn =^> btn.classList.remove('active'^)^);
